<h1>My Awesome list.html.erb of Songs</h1>
<table>
  <tr>
    <th>Number</th>
    <th>Name</th>
    <th>Duration</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
  <% @songs.each do |song| %>
    <tr>
      <td><%= song.id %></td>
      <td><%= song.name %></td>
      <td><%= song.duration %></td>
      <td><%= link_to "Edit", edit_song_path(song.id) %></td>
      <td><%= link_to "Delete", delete_song_path(song.id), method: 'delete' %></td>
    </tr>
  <% end %>
</table>
